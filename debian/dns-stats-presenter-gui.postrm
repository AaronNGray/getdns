#!/bin/sh
set -e

if test "$1" = "purge"; then
    # this could be wrapped inside debconf question whether to remove the data or not
    rm -rf /usr/share/dns-stats-presenter/www
    rm -rf /usr/share/dns-stats-presenter

    rm -rf /var/lib/dns-stats-presenter/www
    dpkg-statoverride --remove /var/lib/dns-stats-presenter/www >/dev/null 2>/dev/null || true
fi

