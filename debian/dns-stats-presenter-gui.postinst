#!/bin/sh
set -e

# Make /var/lib/dns-stats-presenter/data writeable by www-data:www-data user
if ! dpkg-statoverride --list /var/lib/dns-stats-presenter/www >/dev/null; then
  dpkg-statoverride --update --add www-data www-data 0755 /var/lib/dns-stats-presenter/www >/dev/null 2>/dev/null || true
fi

#DEBHELPER#
